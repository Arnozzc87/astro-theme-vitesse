# API配置
PORT=3000

# AI提供商配置
AI_PROVIDER=deepseek  # 可选: openai, deepseek

# DeepSeek配置
DEEPSEEK_API_KEY=your_deepseek_api_key

# OpenAI配置（备用）
OPENAI_API_KEY=your_openai_api_key

# SEO增强配置
ENABLE_COMPETITIVE_ANALYSIS=true  # 是否启用竞争分析

# 网站配置
WEBSITE_URL=https://weipei.edu.cn

# 部署类型(github, netlify, vercel, aliyun)
DEPLOY_TYPE=aliyun

# 阿里云服务器配置（使用阿里云服务器）
# 构建方法：ssh, webhook, local
ALIYUN_BUILD_METHOD=ssh
# SSH方式配置
ALIYUN_SSH_HOST=your_aliyun_host
ALIYUN_SSH_PORT=22
ALIYUN_SSH_USER=your_username
ALIYUN_SSH_KEY=your_private_key_content
ALIYUN_BUILD_SCRIPT=cd /path/to/website && git pull && npm install && npm run build
# Webhook方式配置
ALIYUN_WEBHOOK_URL=https://your_server.com/webhook/build
ALIYUN_WEBHOOK_SECRET=your_webhook_secret

# GitHub配置（使用GitHub Actions）
# DEPLOY_TYPE=github
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repo_name
GITHUB_WORKFLOW_ID=auto-build.yml
GITHUB_TOKEN=your_github_token

# Netlify配置（使用Netlify）
# DEPLOY_TYPE=netlify
NETLIFY_SITE_ID=your_netlify_site_id
NETLIFY_TOKEN=your_netlify_token

# Vercel配置（使用Vercel）
# DEPLOY_TYPE=vercel
VERCEL_PROJECT_ID=your_vercel_project_id
VERCEL_TOKEN=your_vercel_token

# 飞书Webhook安全配置
FEISHU_VERIFICATION_TOKEN=your_feishu_verification_token
FEISHU_ENCRYPT_KEY=your_feishu_encrypt_key 
